{
  "$schema": "https://turbo.build/schema.json",
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**"],
      "env": ["BASE_URL"]
    },
    "test": { "dependsOn": ["^build"], "inputs": ["$TURBO_DEFAULT$", "tests"] },
    "cov": { "dependsOn": ["^build"], "inputs": ["$TURBO_DEFAULT$", "tests"] },
    "lint": { "outputs": [] },
    "fmt": { "outputs": [], "cache": false }
  },
  "globalEnv": ["NODE_ENV", "GITHUB_TOKEN"]
}
